          }
        }
      }
    },
    "/api/pnl/{cryptoSymbol}": {
      "get": {
        "tags": [
          "Pnl"
        ],
        "parameters": [
          {
            "name": "cryptoSymbol",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
